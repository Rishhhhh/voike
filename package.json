{
  "name": "voike-x",
  "version": "0.1.0",
  "description": "VOIKE-X kernel-aware hybrid database backend",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p flow/tsconfig.json && tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "start": "node -r tsconfig-paths/register dist/index.js",
    "test": "node --localstorage-file=.jest-localstorage ./node_modules/jest/bin/jest.js",
    "regression": "ts-node -r tsconfig-paths/register scripts/regression.ts",
    "lint": "tsc --noEmit",
    "seed": "ts-node -r tsconfig-paths/register src/scripts/seed.ts",
    "heartbeat": "python scripts/voike_heartbeat.py"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@fastify/cors": "^8.3.0",
    "@fastify/multipart": "^8.3.0",
    "@fastify/websocket": "^8.3.1",
    "bcryptjs": "^2.4.3",
    "csv-parse": "^5.5.5",
    "dotenv": "^16.4.7",
    "eventemitter3": "^5.0.1",
    "fastify": "^4.28.1",
    "file-type": "^18.7.0",
    "jsonwebtoken": "^9.0.2",
    "parquetjs-lite": "^0.8.0",
    "pdf-parse": "^1.1.1",
    "pg": "^8.11.5",
    "pg-format": "^1.0.4",
    "pgvector": "^0.1.8",
    "pino": "^9.3.2",
    "pino-pretty": "^11.2.2",
    "simple-git": "^3.30.0",
    "tsconfig-paths": "^4.2.0",
    "undici": "^6.19.7",
    "uuid": "^11.0.3",
    "xlsx": "^0.18.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^22.7.5",
    "@types/pg": "^8.15.6",
    "csv-generate": "^4.4.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.5.4"
  }
}
